                                                               "E12 Изготовлeниe"@ru ,
                                                               "E12 生产"@zh ;
                                                    <http://www.w3.org/2004/02/skos/core#notation> "E12"@en ;
                                                    <http://www.w3.org/2004/02/skos/core#prefLabel> "Herstellung"@de ,
                                                                                                    "Production"@en ,
                                                                                                    "Production"@fr ,
                                                                                                    "Produção"@pt ,
                                                                                                    "Παραγωγή"@el ,
